{
    "name": "rebelcode/wp-admin-menu",
    "description": "An object-oriented API for WordPress Admin menus.",
    "type": "library",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "RebelCode",
            "email": "dev@rebelcode.com"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require": {
        "php": "^5.4 | ^7.0",
        "rebelcode/wp-admin-page-interface": "dev-task/initial-interfaces",
        "rebelcode/wp-admin-menu-interface": "dev-task/initial-interfaces",
        "dhii/callback-aware-trait": "dev-task/initial-traits",
        "dhii/data-hierarchy-abstract": "dev-task/initial-classes",
        "dhii/data-key-value-aware-trait": "dev-task/initial-traits",
        "dhii/validation-base": "dev-master",
        "dhii/iterator-base": "dev-task/initial-classes"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "ptrofimov/xpmock": "^1.1",
        "dhii/php-cs-fixer-config": "master",
        "codeclimate/php-test-reporter": "<=0.3.2",
        "10up/wp_mock": "^0.2.0"
    },
    "autoload": {
        "psr-4": {
            "RebelCode\\WordPress\\Admin\\Menu\\": "src"
        }
    },
    "scripts": {
        "test": "phpunit",
        "csfix": "php-cs-fixer fix -vvv"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/RebelCode/wp-admin-menu-interface",
            "no-api": true
        },
        {
            "type": "vcs",
            "url": "https://github.com/RebelCode/wp-admin-page-interface",
            "no-api": true
        },
        {
            "type": "vcs",
            "url": "https://github.com/Dhii/callback-aware-trait"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Dhii/data-hierarchy-abstract"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Dhii/data-key-value-aware-trait"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Dhii/psr-14"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Dhii/iterator-interface"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Dhii/iterator-abstract"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Dhii/iterator-base"
        }
    ]
}
