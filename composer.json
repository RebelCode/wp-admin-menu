{
    "name": "rebelcode/wp-admin-menu",
    "description": "An object-oriented API for WordPress Admin menus.",
    "type": "library",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "RebelCode",
            "email": "dev@rebelcode.com"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require": {
        "php": "^5.4 | ^7.0",
        "rebelcode/wp-admin-page-interface": "dev-develop",
        "rebelcode/wp-admin-menu-interface": "dev-task/initial-interfaces",
        "dhii/callback-abstract": "^0.1-alpha1",
        "dhii/data-hierarchy-abstract": "dev-task/initial-classes",
        "dhii/data-key-value-aware-abstract": "^0.1-alpha3",
        "dhii/validation-base": "^0.1-alpha1 | ^0.2-alpha1",
        "dhii/iterator-base": "dev-develop"
    },
    "require-dev": {
        "php": "^5.6 | ^7.0",
        "phpunit/phpunit": "^4.8",
        "ptrofimov/xpmock": "^1.1",
        "dhii/php-cs-fixer-config": "master",
        "codeclimate/php-test-reporter": "<=0.3.2",
        "10up/wp_mock": "^0.2.0"
    },
    "autoload": {
        "psr-4": {
            "RebelCode\\WordPress\\Admin\\Menu\\": "src"
        }
    },
    "scripts": {
        "test": "phpunit",
        "csfix": "php-cs-fixer fix -vvv"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Dhii/data-hierarchy-abstract"
        }
    ]
}
